  # the "-P none" prevents password-encrypted zip files from hanging
  # the "-q" stops promps for user interaction.
  CmdPre: "-q -P none -o"
  CmdPost: "-x / >/dev/null 2>&1"
  MetaCmd: ""
  Type: CMD_ARC
  Status: 1
  ModeMaskDir: 0177000
  ModeMaskReg: 0177000
  MetaCmd: "unzip -Zhzv '%s' > '%s'"
